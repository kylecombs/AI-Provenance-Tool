{
  "include": [
    "app"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "venv"
  ],
  "venvPath": ".",
  "venv": "venv",
  "pythonVersion": "3.10",
  "executionEnvironments": [
    {
      "root": "app",
      "pythonVersion": "3.10",
      "pythonPlatform": "All"
    }
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": false,
  "reportMissingTypeStubs": false
}